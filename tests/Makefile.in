PREFIX = @prefix@
CC = @CC@
CFLAGS = @CFLAGS@ -I. -I../../../include 
LDFLAGS = @LDFLAGS@
LIBS = -lpablio @LIBS@ 
AR = @AR@
RANLIB = @RANLIB@
INSTALL = @INSTALL@
SHARED_FLAGS = @SHARED_FLAGS@
DLL_LIBS = @DLL_LIBS@
TOOLROOT=@TOOLROOT@
LOCALOPT=@LOCALOPT@
LOCALLINK=@LOCALLINK@
LOCALDLINK=@LOCALDLINK@
DLLTOOL=@DLLTOOL@


SOURCES = \
	test_rw.c \
	test_rw_echo.c \
	test_w_saw8.c \
	test_w_saw.c \
	passthru.c \
	sine.c

EXECS = $(SOURCES:%.c=../bin/%@EXEEXT@)

all: $(EXECS)

$(EXECS): ../bin/%@EXEEXT@: Makefile %.c
	$(CC) -o $@ $(CFLAGS) $*.c -L../../../lib $(LIBS) $(LDFLAGS) $(LOCALOPT) $(LOCALLINK)

clean:
	echo No intermediate files here 

distclean: clean
	cd ../bin;$(RM) -f $(EXECS);cd ../tests

%.@OBJEXT@: %.c Makefile $(PAINC)
	$(CC) -c $(CFLAGS) $< -o $@ $(LOCALOPT)
